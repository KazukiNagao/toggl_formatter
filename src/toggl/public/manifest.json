{
    "manifest_version": 2,
    "name": "Toggl Formatter",
    "version": "0.1.0",
    "description": "Fetch Toggl today's report and format them.",
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "browser_action": {
        "default_title": "Toggl Formatter",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}